{
  "name": "official-storybook",
  "version": "4.0.0-alpha.2",
  "private": true,
  "scripts": {
    "build-storybook": "build-storybook -c ./ -s built-storybooks",
    "chromatic": "chromatic test --storybook-addon --exit-zero-on-changes --app-code ab7m45tp9p",
    "generate-addon-jest-testresults": "jest --config=tests/addon-jest.config.json --json --outputFile=stories/addon-jest.testresults.json",
    "image-snapshots": "yarn run build-storybook && yarn run do-image-snapshots",
    "do-image-snapshots": "../../node_modules/.bin/jest --projects=./image-snapshots",
    "storybook": "start-storybook -p 9011 -c ./ -s built-storybooks"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "4.0.0-alpha.2",
    "@storybook/addon-actions": "4.0.0-alpha.2",
    "@storybook/addon-backgrounds": "4.0.0-alpha.2",
    "@storybook/addon-events": "4.0.0-alpha.2",
    "@storybook/addon-info": "4.0.0-alpha.2",
    "@storybook/addon-jest": "4.0.0-alpha.2",
    "@storybook/addon-knobs": "4.0.0-alpha.2",
    "@storybook/addon-links": "4.0.0-alpha.2",
    "@storybook/addon-notes": "4.0.0-alpha.2",
    "@storybook/addon-options": "4.0.0-alpha.2",
    "@storybook/addon-storyshots": "4.0.0-alpha.2",
    "@storybook/addon-storysource": "4.0.0-alpha.2",
    "@storybook/addon-viewport": "4.0.0-alpha.2",
    "@storybook/addons": "4.0.0-alpha.2",
    "@storybook/components": "4.0.0-alpha.2",
    "@storybook/node-logger": "4.0.0-alpha.2",
    "@storybook/react": "4.0.0-alpha.2",
    "babel-runtime": "^6.26.0",
    "enzyme-to-json": "^3.3.3",
    "eventemitter3": "^3.0.1",
    "format-json": "^1.0.3",
    "global": "^4.3.2",
    "paths.macro": "^2.0.2",
    "prop-types": "^15.6.1",
    "react": "^16.3.2",
    "react-chromatic": "^0.8.1",
    "react-dom": "^16.3.2",
    "uuid": "^3.2.1"
  }
}
